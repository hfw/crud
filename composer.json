{
  "name": "hfw/db",
  "type": "library",
  "description": "Database CRUD by annotation",
  "keywords": [
    "database",
    "crud",
    "annotation",
    "migration"
  ],
  "homepage": "https://github.com/hfw/db",
  "license": "MIT",
  "require": {
    "php": "~7.4",
    "ext-pdo": "*"
  },
  "bin": [
    "bin/helix.db.migrate.php"
  ],
  "autoload": {
    "psr-4": {
      "Helix\\": "src/",
      "Helix\\DB\\": "src/DB/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "test/src/"
    }
  }
}
